{
    "manifest_version": 2,

    "name": "Slack real names + handles",
    "description": "Display slack handles alongside real names.",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [ {
        "js": [ "script.js" ],
        "matches": [ "https://*.slack.com/messages/*" ]
    } ]
}
